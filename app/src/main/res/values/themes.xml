<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.VideoStreaming" parent="Theme.MaterialComponents">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue_grey_800</item>
        <item name="colorPrimaryVariant">@color/blue_grey_900</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/neon_green_300</item>
        <item name="colorSecondaryVariant">@color/neon_green_500</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Surface color. -->
        <item name="colorSurface">@color/blue_grey_800</item>
        <item name="colorOnSurface">@color/white</item>
        <item name="android:colorBackground">@color/blue_grey_900</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>